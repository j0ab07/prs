components:
  schemas:
    AccessLog:
      type: object
      properties:
        log_id:
          type: integer
          readOnly: true
        official:
          type: integer
        action:
          type: string
          maxLength: 100
        timestamp:
          type: string
          format: date-time
          readOnly: true
      required:
      - action
      - log_id
      - official
      - timestamp
    CriticalItem:
      type: object
      properties:
        item_id:
          type: integer
          readOnly: true
        name:
          type: string
          maxLength: 100
        purchase_limit:
          type: integer
          maximum: 9223372036854775807
          minimum: -9223372036854775808
          format: int64
        limit_period:
          type: string
          maxLength: 20
        allowed_purchase_day:
          type: string
          nullable: true
          maxLength: 20
      required:
      - item_id
      - limit_period
      - name
      - purchase_limit
    GovernmentOfficial:
      type: object
      properties:
        official_id:
          type: integer
          readOnly: true
        username:
          type: string
          readOnly: true
        password_hash:
          type: string
          readOnly: true
        role:
          type: string
          maxLength: 50
        created_at:
          type: string
          format: date-time
          readOnly: true
      required:
      - created_at
      - official_id
      - password_hash
      - role
      - username
    Individual:
      type: object
      properties:
        prs_id:
          type: string
          readOnly: true
        national_identifier:
          type: string
          maxLength: 255
        date_of_birth:
          type: string
          format: date
        created_at:
          type: string
          format: date-time
          readOnly: true
      required:
      - created_at
      - date_of_birth
      - national_identifier
      - prs_id
    Merchant:
      type: object
      properties:
        merchant_id:
          type: integer
          readOnly: true
        business_license:
          type: string
          maxLength: 255
        name:
          type: string
          maxLength: 100
        address:
          type: string
          maxLength: 255
        created_at:
          type: string
          format: date-time
          readOnly: true
      required:
      - address
